<krpano version="1.17" title="Virtual Tour" onstart="startup();">





<action name="startup">
 global_actions();
        if(startscene === null, copy(startscene,scene[scene_facade].name));
        loadscene(get(startscene), null, MERGE);
        set(heading, 65);               
    </action>
	
	
<!-- Sample Blend Script use seconds if required blend(seconds),"blend(7)" -->
<!-- <events name="blend" onloadcomplete="blendhotspot(pano_name);blend()"/>     -->
<!-- <events name="blend_mul3" onloadcomplete="blendhotspot_mul3(pano_name1,pano_name2);blend_mul3());"/> -->
<!-- <events name="blend_mul4" onloadcomplete="blendhotspot_mul4(pano_name1,pano_name2,pano_name3);blend_mul4());"/> -->

	
<autorotate enabled="true"
            waittime="5.0"
            accel="1.0"
            speed="3.0"
            horizon="0.0"
            tofov="off"
            />
        
         <display fps="60"
         details="16"
         tessmode="-1"
         movequality="HIGH"
         stillquality="HIGH"
         flash10="on"
         movequality10="HIGH"
         stillquality10="HIGH"
         sharpen="12"
         stilltime="0.25"
         showpolys="false"
         />	
         
         
                   <style name="video"  distorted="true"
         url="plugins/videoplayer.swf" 
         alturl="plugins/videoplayer.js" enabled="false"        
         handcursor="false" 
         pausedonstart="true"
         zorder="2"
         volume="0.3" alpha="0"
         edge="lefttop"
         directionalsound="false"
         rangefalloff="1.0"
         outofrangevolume="0.0"        
         loop="true"        
         onclick="togglepause()"
         />   
         
         
         
             <plugin name="orientation" 
        url="plugins/orientation.js" 
        keep="true"
        devices="HTML5"
        onorientationchange="updateorientation();"
        />
        

    
      <events name="orientation" keep="true" devices="html5"
onloadcomplete="updateorientation();"
            />


<action name="updateorientation">

 if(isHTML5,

if(plugin[orientation].portrait == 1,
set(layer[rotate_device1].alpha, 1.0); 
delayedcall(3.0, tween(layer[rotate_device1].alpha, 0.0, 1.0); );
,
tween(layer[rotate_device1].alpha, 0.0, 0.0); );
 
 
   );

      );

          
      
        
  
           
  
</action>

    <layer name="rotate_device1" keep="true" devices="Mobile|ipad|iphone"
           url="skin/rotate_device.png" scale="0.5"
           align="top" edge="center" y="28%"
           alpha="1" autoalpha="1"
           enabled="false"
           />




  
        
           <action name="orientation">
              
                    tween(layer[webvr_rotate_to_landscape_request].alpha, 0.0, 0.0);
                  ,
                    delayedcall(3.0, tween(layer[webvr_rotate_to_landscape_request].alpha, 0.0, 1.0); );
                  );
              );
          ,
            set(lastaspect, 0);
          );
    </action>
         
         
         
         
         <style name="cube-hs-video" keep="false" distorted="true" width="1000" height="1000" zorder="9" enabled="false" alpha="1" />  
         
         
<style name="skin_hotspotstyle" url="images/hotspot.png" scale="0.40" zorder="10"  onover="tween(scale,0.45)" onout="tween(scale,0.35)"  onclick="if(linkedscene, looktohotspot(); loadscene(get(linkedscene),null,MERGE,BLEND(1)); );" distorted="true" onhover="showtext(get(scene[get(linkedscene)].title),alttext));" />
